<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.revenant.takego">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controller.AddBranchActivity"
            android:label="@string/add_branch" />
        <activity android:name=".controller.ShowAllActivity" />
        <activity android:name=".controller.AddCarActivity" />
        <activity android:name=".controller.AddModelActivity" />
        <activity android:name=".controller.AddCustomerActivity" />
        <activity android:name=".controller.AddReservationActivity"></activity>
    </application>

</manifest>